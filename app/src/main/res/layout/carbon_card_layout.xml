<?xml version="1.0" encoding="utf-8"?>
<carbon.widget.LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <carbon.widget.LinearLayout
        android:background="@color/black"
        android:layout_margin="@dimen/_16sdp"
        android:padding="@dimen/_16sdp"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_255sdp">
        <carbon.widget.LinearLayout
            app:carbon_cornerRadius="@dimen/_16sdp"
            app:carbon_elevation="@dimen/_4sdp"
            app:carbon_elevationShadowColor="#F39DC3"
            android:background="@color/white"
            android:layout_width="@dimen/_64sdp"
            android:layout_height="@dimen/_64sdp"></carbon.widget.LinearLayout>
    </carbon.widget.LinearLayout>
</carbon.widget.LinearLayout>